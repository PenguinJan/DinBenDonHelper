{
    "name": "Dinbendon Helper",
    "description": "Dinbendon 網站輔助工具",
    "version": "1.1",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "tabs",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://dinbendon.net/do/order*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}